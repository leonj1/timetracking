{
  "manifest_version": 3,
  "name": "Team Manager Extension",
  "version": "1.1",
  "description": "An extension to manage teams and say Hello.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["teams.html", "people.html", "set_date.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
